{
  "name": "larabug/larabug",
  "description": "Laravel 5.8/6.x bug notifier",
  "keywords": [
    "laravel",
    "log",
    "error"
  ],
  "require": {
    "php": ">=7.1.3",
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/support": "5.8.*|^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^3.8 || ^4.0"
  },
  "autoload": {
    "psr-4": {
      "LaraBug\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaraBug\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaraBug\\ServiceProvider::class"
      ]
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Dennis Smink",
      "email": "dennissmink@gmail.com",
      "homepage": "http://www.dennissmink.nl",
      "role": "Owner"
    }
  ]
}
